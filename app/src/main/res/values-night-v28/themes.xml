<resources>

    <style name="containersCar">
        <item name="android:background">@color/md_theme_dark_primaryContainer</item>
        <item name="android:elevation">1dp</item>
        <item name="android:outlineSpotShadowColor">@color/md_theme_dark_shadow</item>
        <item name="android:outlineProvider">background</item>
    </style>

    <style name="containersImage">
        <item name="android:elevation">5dp</item>
        <item name="android:outlineProvider">background</item>
        <item name="android:outlineAmbientShadowColor">@color/md_theme_dark_shadow</item>
        <item name="android:outlineSpotShadowColor">@color/md_theme_dark_shadow</item>
    </style>

    <style name="floatingActionButton" patern="Widget.Design.FloatingActionButton">
        <item name="elevation">3dp</item>
        <item name="rippleColor">@color/md_theme_dark_surfaceTint</item>
        <item name="hoveredFocusedTranslationZ">1dp</item>
        <item name="android:backgroundTint">@color/md_theme_dark_primaryContainer</item>
        <item name="tint">@color/md_theme_dark_onPrimaryContainer</item>
        <item name="android:outlineProvider">background</item>
        <item name="android:outlineSpotShadowColor">@color/md_theme_dark_shadow</item>
        <item name="borderWidth">0dp</item>
    </style>
</resources>